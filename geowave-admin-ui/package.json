{
  "name": "geowave-admin-ui",
  "version": "1.0.0",
  "description": "Administrative console for a GeoWave deployment",
  "repository": "https://github.com/radiant-maxar/geowave-admin-ui",
  "author": "Radiant Solutions",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/classnames": "2.2.3",
    "@types/enzyme": "3.1.9",
    "@types/jest": "22.1.3",
    "@types/react": "16.3.5",
    "@types/react-dom": "16.0.4",
    "@types/react-router-dom": "4.2.4",
    "@types/react-test-renderer": "16.0.1",
    "awesome-typescript-loader": "5.0.0",
    "css-loader": "0.28.10",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "file-loader": "1.1.9",
    "html-webpack-plugin": "3.2.0",
    "jest": "22.4.3",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "react-test-renderer": "16.3.1",
    "style-loader": "0.20.2",
    "ts-jest": "22.4.2",
    "tslint": "5.9.1",
    "tslint-loader": "3.6.0",
    "typescript": "2.9.2",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3"
  },
  "dependencies": {
    "axios": "0.18.0",
    "classnames": "2.2.5",
    "core-js": "2.5.3",
    "mobx": "4.1.1",
    "mobx-react": "5.0.0",
    "moment": "2.22.1",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-router-dom": "4.2.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*(\\.test|Test)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/test/file-mock.ts",
      "\\.(css|less)$": "<rootDir>/test/style-mock.ts"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/enzyme-config.ts"
  }
}
